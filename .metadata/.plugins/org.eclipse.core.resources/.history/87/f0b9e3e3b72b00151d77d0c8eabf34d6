package datastructure.tree;

/**
 * @Description 二叉树的抽象数据类型
 * @author mastery
 * @Date 2015年7月16日下午8:28:13
 * @param <T>
 */
public interface BinaryTree<T> {

	BinaryTree<T> insertLeftNode(T data);

	BinaryTree<T> insertRightNode(T data);

	BinaryTree<T> deleteLeftNode(T data);

	BinaryTree<T> deleteRightNode(T data);
	
	/**
	 * 按某种次序访问当前二叉树的每个结点，且每个结点只访问一次，访问结点时要实现的具体操作
	 * 由TraverseStrategy接口的实现类完成。
	 * 
	 * @param strategy
	 * @return
	 */
	String[] traverse(TraverseStrategy strategy);
}

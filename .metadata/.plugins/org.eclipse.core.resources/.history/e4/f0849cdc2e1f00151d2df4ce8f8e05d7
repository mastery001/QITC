package datastructure.linear.linked;

import static org.junit.Assert.fail;

import org.junit.Test;

import datastructure.exception.StructureException;
import datastructure.linear.List;

public class BidirectionalLinkedListTest {

	@Test
	public void testBidirectionalLinkedList() throws StructureException {
		List<Integer> list = new BidirectionalLinkedList<Integer>();
		for(int i = 0 ; i < 10 ; i ++) {
			list.insert(i, i+1);
		}
		list.delete(9);
		for(int i = 0 ; i < list.size() ; i++) {
			System.out.print(list.get(i) + "   ");
		}
	}


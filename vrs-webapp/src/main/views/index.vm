                            </li>
                    #end	
                    <li>
                        <a href="../login/logout.action">注销</a>
                    </li>
                </ul>
            </div>
        </div>
        <div id="leftside">
            <div id="sidebar_s">
                <div class="collapse">
                    <div class="toggleCollapse">
                        <div></div>
                    </div>
                </div>
            </div>
            <div id="sidebar">
                <div class="toggleCollapse">
                    <h2>VRS系统</h2>
                    <div>收缩</div>
                </div>
                <div class="accordion" fillSpace="sidebar">
                    <!------------------------------------ 视频管理开始 ---------------------------------->
                    <div class="accordionHeader">
                        <h2>
                            <span>Folder</span>
                            视频管理</h2>
                    </div>
                    <div class="accordionContent">
                        #foreach($!info in $!channelList)
                            <ul class="tree treeFolder collapse">
                                #if ($!info.id != -1)
                                    <li>
                                        <a>$!info.name</a>
                                        <ul>
                                            <li>
                                                <a href="../album/toEdit.action?cid=$!info.id" target="navTab" rel="addAlbum$!info.id" title="新增$!info.name专辑">新增专辑</a>
                                            </li>
                                            <li>
                                                <a href="../album/view.action?orderBy=1&cid=$!info.id" target="navTab" rel="albumList$!info.id" title="$!info.name专辑列表">专辑列表</a>
                                            </li>
                                        </ul>
                                    </li>
                                #end	
                            </ul>
                        #end
                        <ul class="tree treeFolder"></ul>
                    </div>
                    <!------------------------------------ 视频管理结束 ---------------------------------->
                    <div class="accordionHeader">
                        <h2>
                            <span>Folder</span>
                            系统管理</h2>
                    </div>
                    <div class="accordionContent">
                        <ul class="tree treeFolder">
							 <li>
                                <a href="../channel/list.action" target="navTab" rel="rightcontent">频道管理</a>
                            </li>
                        </ul>
                    </div>
                    <div class="accordionHeader">
                        <h2>
                            <span>Folder</span>
                            日志系统</h2>
                    </div>
                    <div class="accordionContent">
                        <ul class="tree treeFolder">
                            <li>
                                <a href="../log/dataManaged.action" target="navTab" rel="rightcontent">日志查询</a>
                            </li>
                        </ul>
                    </div>
                    <div class="accordionHeader">
                        <h2>
                            <span>Folder</span>
                            监控系统</h2>
                    </div>
                    <div class="accordionContent">
                        <ul class="tree treeFolder">
                            <li>
                                <a href="http://10.182.192.98:28080/monitorjob/search.action" target="navTab" rel="rightcontent">监控管理</a>
                            </li>
                            <li>
                                <a href="http://10.182.192.98:28080/monitorreport/search.action" target="navTab" rel="rightcontent">监控查询</a>
                            </li>
                        </ul>
                    </div>

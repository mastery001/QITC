  <script>
function validate(f)
{

	var username=f.username.value;
	var pass=f.pass.value;
	if(username.value=="")
	{
		alert("请输入用户名");
		username.focus();
		return;
	}
	if(pass.value=="")
	{
		alert("请输入密码");
		return;
	}
return true;
}
</script>
